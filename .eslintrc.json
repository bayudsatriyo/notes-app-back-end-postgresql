{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "class-methods-use-this": "off",
        "no-underscore-dangle": 0,
        "no-undef": "off",
        "no-unused-vars": "off",
        "eslint-disable-next-line camelcase": false
    }
}
